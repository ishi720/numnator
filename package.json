{
  "name": "numnator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "./node_modules/.bin/vue-cli-service serve",
    "build": "./node_modules/.bin/vue-cli-service build",
    "lint": "./node_modules/.bin/vue-cli-service lint",
    "postinstall": "yarn build",
    "start": "node server.js",
    "test": "jest --version & jest --coverage --no-cache",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "babel-jest": "27.x.x",
    "core-js": "2.x.x",
    "express": "4.x.x",
    "jest": "29.x.x",
    "vue": "2.x.x",
    "vue-router": "3.x.x"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.x.x",
    "@vue/cli-plugin-eslint": "3.x.x",
    "@vue/cli-service": "3.x.x",
    "babel-eslint": "10.x.x",
    "coveralls": "3.x.x",
    "eslint": "5.x.x",
    "eslint-plugin-vue": "5.x.x",
    "vue-template-compiler": "2.x.x"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
